<div class="crud-example-list">
  <h2>{{ 'Items List'|t }}</h2>
  {% if items %}
    <ul>
      {% for item in items %}
        <li>
          <strong>{{ item.name }}</strong><br>
          {{ item.description }}
        </li>
      {% endfor %}
    </ul>
  {% else %}
    <p>{{ 'No items found.'|t }}</p>
  {% endif %}
</div>
